		"three": "^0.153.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.33.0"
	},
	"type": "module",
	"dependencies": {
		"@iconify/json": "^2.2.83",
		"@lucia-auth/adapter-prisma": "^2.0.0",
		"@lucia-auth/sveltekit": "^0.6.11",
